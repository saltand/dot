    {
      "after": ["\"", "_", "s"],
      "before": ["s"]
    },
    {
      "after": ["\"", "_", "x"],
      "before": ["x"]
    },
    {
      "after": ["s"],
      "before": ["\\", "s"]
    },
    {
      "after": ["^"],
      "before": ["H"]
    },
    {
      "after": ["g", "_"],
      "before": ["L"]
    },
    {
